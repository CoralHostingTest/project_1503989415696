namespace com.yahoo.ecosystem.mobile_payment.parsec
name Wallet
version 1
base "/mobilePayment"

resource string POST "/wallet" (name=postWallet) {
    string body;
    string sign (header="Req-Msg-Sign")

    expected OK;
    exceptions {
        ResourceError INTERNAL_SERVER_ERROR;
        ResourceError BAD_REQUEST;
        ResourceError UNAUTHORIZED;
        ResourceError FORBIDDEN;
    }
}
